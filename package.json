{
  "name": "apollo-challenge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-cli serve --config webpack.dev.js",
    "build": "webpack-cli --config webpack.prod.js",
    "types:check": "yarn run tsc --noEmit",
    "lint": "eslint --max-warnings 0 \"src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint ./src --ext .json --ext .js",
